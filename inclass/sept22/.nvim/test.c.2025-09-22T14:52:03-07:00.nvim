#include <stdio.h>
#include <stdlib.h>
#include <sys/wait.h>
#include <unistd.h>

int main() {
  int x = 0;
  pid_t b = fork();
  waitpid(b, &x, 0);
  if(b != 0 )
    execl("/bin/ls", "/bin/ls", "../../", "-a", NULL);
  else
  printf("child\n");
  execl("/bin/ls", "/bin/ls", "../../", "-alh", NULL);
  return 0;
};
