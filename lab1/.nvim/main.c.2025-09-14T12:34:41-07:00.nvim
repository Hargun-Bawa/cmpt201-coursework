#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
  char *line = NULL;
  size_t readd;
  size_t len = 32;
  line = (char *)malloc(len * sizeof(char));
  if (line == NULL) {
    perror("Unable to allocate");
    exit(1);
  }
  printf("Enter Tokens\n");
  readd = getline(&line, &len, stdin);
  printf("'%s'\n", line);
  strtok_r(line, " ");

  return 0;
};
