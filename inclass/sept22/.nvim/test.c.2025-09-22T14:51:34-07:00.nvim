#include <stdio.h>
#include <stdlib.h>
#include <sys/wait.h>
#include <unistd.h>

int main() {
  int x = 0;
  pid_t b= fork();
  waitpid(b, &x, 0);
  execl("/bin/ls", "/bin/ls", "../../", "-a", NULL);
  sleep(3);
  printf("child\n");
  execl("/bin/ls", "/bin/ls", "../../", "-alh", NULL);
  return 0;
};
