#include <stdio.h>
#include <stdlib.h>
#include <sys/wait.h>
#include <unistd.h>

int main() {
  int x = 0;
  pid_t b = fork();
  waitpid(b, &x, 0);
  if (b != 0)
    printf("I am the parent");
    execl("/bin/ls", "/bin/ls", "../../", "-a", NULL);
  else
    printf("I am the child");
    execl("/bin/ls", "/bin/ls", "../../", "-alh", NULL);
  return 0;
};
